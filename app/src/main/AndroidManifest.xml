<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gdgthess.droidcongr" >

    <meta-data
        android:name="com.google.android.gms.version"
        android:value="102909531751-qo3k04rmbfhgocro2boerbk9c7jp3nmg.apps.googleusercontent.com" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationDrawerFragment"
            android:label="@string/title_activity_navigation_drawer_fragment" >
        </activity>
        <activity
            android:name=".StartActivity"
            android:label="@string/title_activity_start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TicketsActivity"
            android:label="@string/title_activity_tickets" >
        </activity>
        <activity
            android:name=".PartnersActivity"
            android:label="@string/title_activity_partners" >
        </activity>
        <activity
            android:name=".TeamActivity"
            android:label="@string/title_activity_team" >
        </activity>
        <activity
            android:name=".LineupActivity"
            android:label="@string/title_activity_lineup" >
        </activity>
        <activity
            android:name=".BarcampActivity"
            android:label="@string/title_activity_barcamp" >
        </activity>
        <activity
            android:name=".SessionsActivity"
            android:label="@string/title_activity_sessions" >
        </activity>
    </application>

</manifest>
